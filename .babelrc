{
    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["import", { "libraryName": "ant-design-vue", "libraryDirectory": "es", "style": "css" }]
    ]
}
